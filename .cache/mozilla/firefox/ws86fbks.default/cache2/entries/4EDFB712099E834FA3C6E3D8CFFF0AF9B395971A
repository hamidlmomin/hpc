    background-color:#dbdbdb;
    border:1px solid #cfcfcf;
    border-radius: 4px;
}

.column-content-tip a.x-grid3-hd-btn {
    display: none;
}

/* This is for customize file field that indicates a configured value is present. */
.has-configured-value {
    background-color: #c8caca;
}

.printing .x-tool {
    visibility: hidden;
}

